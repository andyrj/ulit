{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "max-classes-per-file": false,
        "no-empty": false,
        "no-bitwise": false,
        "no-null-keyword": false,
        "interface-name": false,
        "no-implicit-dependencies": false,
        "no-debugger": false,
        "no-string-literal": false,
        "no-unnecessary-initializer": false
    },
    "rulesDirectory": [],
    "exclude": [
        "test"
    ]
}